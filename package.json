{
  "name": "react-unleash-flags",
  "version": "1.1.2",
  "description": "React component for Unleash or Gitlab Feature Flags",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvoordeckers/react-unleash-flags.git"
  },
  "keywords": [
    "react",
    "typescript",
    "unleash",
    "gitlab",
    "feature",
    "flags",
    "component"
  ],
  "author": "Frederik Voordeckers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fvoordeckers/react-unleash-flags/issues"
  },
  "homepage": "https://github.com/fvoordeckers/react-unleash-flags#readme",
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "jest": "^26.4.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
